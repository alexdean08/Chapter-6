public class Song{
    private String title = "";
    private String artist = "";
    private int length = 0;

    public Song(String t, String a, int l){
        title = t;
        artist = a;
        length = l;
    }
    
    public void setTitle(String newT){
        title = newT;
    }

    public String getTitle(){
        return title;
    }
    
    public void setArtist(String newA){
        artist = newA;
    }
    
    public String getArtist(){
        return artist;
    }
    
    public void setLength(int newL){
        length = newL;
    }
    
    public int getLength(){
        return length;
    }
    
    public String toString(){
        return getTitle() + " by " + getArtist() + " that lasts " + getLength() + " seconds";
    }
}
